{
  "name": "triggerator-backend",
  "version": "1.8.0",
  "description": "Google DV360 Triggerator backend (decision engine)",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "test": "mocha --ui tdd --require ts-node/register --require src/test/fixtures-auth-key.ts --timeout 10s src/test/**/*.ts",
    "test:coverage": "nyc --reporter=text mocha --ui tdd --require ts-node/register --require src/test/fixtures-auth-key.ts --timeout 10s src/test/**/*.ts",
    "test:prod": "mocha --ui tdd --require dist/test/fixtures-auth-key.js --timeout 10s dist/test/**/*.js",
    "tsc": "tsc",
    "build": "npm run tsc",
    "start": "node dist/index.js",
    "start:local": "ts-node src/index.ts --keyFile=./keys/triggerator-sd-sa.json --masterSpreadsheet=1fnxjMudPpiHz0XezAmdFjvD2QCYi6IvgluDLpxYs8-M --mailerConfigPath=mailer.sendgrid.json",
    "start:local:js": "node dist/index.js --keyFile=./keys/triggerator-sd-sa.json",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js",
    "deploy": "gcloud app deploy"
  },
  "keywords": [
    "google",
    "triggerator",
    "ads",
    "dv360",
    "dv360api",
    "doubleclick",
    "display & video 360"
  ],
  "author": "Google Inc. (gTech gPS CSE team)",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^5.9.0",
    "@google-cloud/debug-agent": "^5.2.8",
    "@google-cloud/logging-winston": "^4.1.1",
    "@google-cloud/storage": "^5.14.2",
    "@js-joda/core": "^4.0.0",
    "csv": "^5.5.3",
    "csv-parse": "^4.16.3",
    "express": "^4.17.1",
    "gaxios": "^4.3.2",
    "google-auth-library": "^7.9.2",
    "googleapis": "^87.0.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "iconv-lite": "^0.6.3",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "mathjs": "^9.4.5",
    "nodemailer": "^6.6.3",
    "sprintfjs": "^1.2.17",
    "update": "^0.7.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yargs": "^17.1.1",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.173",
    "@types/mathjs": "^9.4.1",
    "@types/mocha": "^9.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/server-destroy": "^1.0.1",
    "@types/uuid": "^8.3.1",
    "@types/yauzl": "^2.9.2",
    "@types/yazl": "^2.4.2",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "open": "^8.2.1",
    "server-destroy": "^1.0.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "// devDependencies": {
    "mocha": "Needed for unit test",
    "nyc": "Needed for test coverage colculation (see npm scripts here)",
    "open": "Needed for auto opening a url in the default browser",
    "ts-node": "Needed for directly require ts module in mochas",
    "shx": "Needed for development-time package.json scripts",
    "nodemon": "Needed for development-time auto-reload server on file changes"
  }
}
