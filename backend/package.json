{
  "name": "triggerator-backend",
  "version": "0.1.0",
  "description": "Google DV360 Triggerator backend (decision engine)",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "test": "mocha --ui tdd --require ts-node/register --require src/test/fixtures-auth-key.ts --timeout 10s src/test/**/*.ts",
    "test-coverage": "nyc --reporter=text mocha --ui tdd --require ts-node/register --require src/test/fixtures-auth-key.ts --timeout 10s src/test/**/*.ts",
    "test:prod": "mocha --ui tdd --require dist/test/fixtures-auth-key.js --timeout 10s dist/test/**/*.js",
    "tsc": "tsc",
    "build": "npm run tsc",
    "start": "node dist/index.js",
    "start:local": "ts-node src/index.ts --keyFile=./keys/triggerator-sd-sa.json --masterSpreadsheet=1fnxjMudPpiHz0XezAmdFjvD2QCYi6IvgluDLpxYs8-M",
    "start:local:js": "node dist/index.js --keyFile=./keys/triggerator-sd-sa.json",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js",
    "deploy": "gcloud app deploy"
  },
  "keywords": [
    "google",
    "triggerator",
    "ads",
    "dv360",
    "dv360api",
    "doubleclick",
    "display & video 360"
  ],
  "author": "Google Inc. (gTech gPS CSE team)",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "@google-cloud/scheduler": "^2.2.0",
    "@google-cloud/storage": "^5.7.4",
    "csv": "^5.3.2",
    "csv-parse": "^4.15.1",
    "express": "^4.17.1",
    "gaxios": "^4.1.0",
    "google-auth-library": "^6.1.6",
    "googleapis": "^67.0.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "iconv-lite": "^0.6.2",
    "install": "^0.13.0",
    "lodash": "^4.17.20",
    "mathjs": "^9.2.0",
    "morgan": "^1.10.0",
    "sprintfjs": "^1.2.17",
    "yargs": "^16.2.0",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/mathjs": "^6.0.11",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/server-destroy": "^1.0.1",
    "@types/yauzl": "^2.9.1",
    "@types/yazl": "^2.4.2",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "open": "^7.4.0",
    "server-destroy": "^1.0.1",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "// devDependencies": {
    "mocha": "Needed for unit test",
    "nyc": "Needed for test coverage colculation (see npm scripts here)",
    "open": "Needed for auto opening a url in the default browser",
    "ts-node": "Needed for directly require ts module in mochas",
    "shx": "Needed for development-time package.json scripts"
  }
}
